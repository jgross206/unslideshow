{
  "name": "My First Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "matches": ["http://www.complex.com/*"],
    "js": ["script.js"]
    }
    ],
  "permissions": [
    "http://www.complex.com/*"
  ]
}